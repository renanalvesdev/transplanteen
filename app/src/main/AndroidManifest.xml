<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.transplanteen.tcc.transplanteen">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.AutenticacaoActivity" />
        <activity android:name=".activity.HomeActivity" />
        <activity android:name=".activity.CadastroEnfermeiroActivity" />
        <activity
            android:name=".activity.EnfermeiroActivity"
            android:label="@string/title_activity_enfermeiro"
            android:theme="@style/AppTheme" />
        <activity android:name=".activity.NovoPacienteActivity" />
        <activity android:name=".activity.PacienteActivity" />
        <activity android:name=".activity.ConsultaActivity"></activity>
    </application>

</manifest>